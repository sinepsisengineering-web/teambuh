# ==========================================
# Шаблон переменных окружения для тенанта
# Заполняется скриптом create-tenant.sh
# ==========================================

# Уникальное имя инстанса (используется в container_name)
INSTANCE_NAME=__TENANT_ID__

# Уникальный внешний порт (автоинкремент: 8001, 8002, ...)
APP_PORT=__PORT__

# Уникальный секрет для JWT (генерируется автоматически)
JWT_SECRET=__SECRET__

# Окружение
NODE_ENV=production
